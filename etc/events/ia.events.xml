<events>
    <event>
        <uei>uei.opennms.org/nodes/nodeStatusServiceChanged</uei>
        <event-label>OpenNMS-defined node event: nodeStatusServiceChanged</event-label>
        <descr>&lt;p&gt;A %service% status change was identified on interface %interface%.&lt;/p&gt; &lt;p&gt;</descr>
        <logmsg dest='logndisplay'>%service% change status identified on interface %interface%.</logmsg>
        <severity>Warn</severity>
    </event>
    <event>
        <uei>uei.opennms.org/nodes/nodeServiceAdded</uei>
        <event-label>OpenNMS-defined node event: nodeServiceAdded</event-label>
        <descr>&lt;p&gt;A %service% was added on interface %interface%.&lt;/p&gt; &lt;p&gt;</descr>
        <logmsg dest='logndisplay'>%service% added on interface %interface%.</logmsg>
        <severity>Warn</severity>
    </event>
    <event>
        <uei>uei.opennms.org/nodes/nodeServiceRemoved</uei>
        <event-label>OpenNMS-defined node event: nodeServiceRemoved</event-label>
        <descr>&lt;p&gt;A %service% was removed on interface %interface%.&lt;/p&gt; &lt;p&gt;</descr>
        <logmsg dest='logndisplay'>%service% removed on interface %interface%.</logmsg>
        <severity>Warn</severity>
    </event>
    <event>
        <uei>uei.opennms.org/applications/applicationLostService</uei>
        <event-label>OpenNMS-defined node event: applicationLostService</event-label>
        <descr>&lt;p&gt; .&lt;/p&gt; &lt;p&gt;</descr>
        <logmsg dest='logndisplay'>Application failure generated by %service% outage identified on interface %interface%.</logmsg>
        <severity>Critical</severity>
    </event>

    <event>
        <uei>uei.opennms.org/internal/inventory/configurationChanged</uei>
        <event-label>IA OpenNMS-defined Inventory changed configuration event.</event-label>
        <descr>&lt;p&gt;Inventory changed configuration event.&lt;/p&gt; &lt;p&gt;</descr>
        <logmsg dest='logndisplay'>&lt;p&gt;Inventory changed configuration event.&lt;/p&gt;</logmsg>
        <severity>Warning</severity>
    </event>

    <event>
        <uei>uei.opennms.org/internal/linkd/macAddressChanged</uei>
        <event-label>IA OpenNMS-defined Linkd changed mac address event.</event-label>
        <descr>&lt;p&gt;Mac address changed on node event.&lt;/p&gt; &lt;p&gt;</descr>
        <logmsg dest='logndisplay'>&lt;p&gt;Mac Address changed on node event.&lt;/p&gt;</logmsg>
        <severity>Warning</severity>
    </event>

    <event>
        <uei>uei.opennms.org/internal/linkd/linkChanged</uei>
        <event-label>IA OpenNMS-defined Linkd changed link event.</event-label>
        <descr>&lt;p&gt;link changed on node event.&lt;/p&gt; &lt;p&gt;</descr>
        <logmsg dest='logndisplay'>&lt;p&gt;Link changed on node event.&lt;/p&gt;</logmsg>
        <severity>Warning</severity>
    </event>

