<?xml version="1.0" encoding="UTF-8"?>
<web-applications xmlns="http://www.opennms.org/xsd/config/http-site-config" 
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                  xsi:schemaLocation="http://www.opennms.org/xsd/config/http-site-config ../../../../../../opennms-config/src/main/castor/web-app-config.xsd ">
  <web-app name="demo">
    <page-sequence name="opennms-login-seq" force-logout="true" >
      <page path="/opennms" port="8080" matches="Password" />
      <page path="/opennms/j_acegi_security_check"  port="8080" method="POST" matches="Log out">
        <parameter key="j_username" value="demo"/>
        <parameter key="j_password" value="demo"/>
      </page>
      <page path="/opennms/event/index.jsp" port="8080" matches="Event Queries" />
      <page path="/opennms/j_acegi_logout" port="8080" matches="logged off" page-type="logout" />
    </page-sequence>
  </web-app>
</web-applications>
