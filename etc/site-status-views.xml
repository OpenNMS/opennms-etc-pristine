<?xml version="1.0" encoding="UTF-8"?>
<site-status-view-configuration 
    xmlns:this="http://www.opennms.org/xsd/config/site-status-views" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.opennms.org/xsd/config/site-status-views http://www.opennms.org/xsd/config/site-status-views.xsd " 
    default-view="default">
  <views>
    <view name="default" >
      <rows>
        <row-def label="Router">
          <category name="Router"/>
        </row-def>
        <row-def label="Firewall">
          <category name="Firewall"/>
        </row-def>
        <row-def label="Switch">
          <category name="Switch"/>
        </row-def>
        <row-def label="Web Server">
          <category name="WebServer"/>
        </row-def>
      </rows>
    </view>
  </views>
</site-status-view-configuration>
